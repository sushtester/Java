		List<WebElement> list= driver.findElements(By.xpath("//ul[@class= 'typeahead__list']/li/a/div/span"));
		
		System.out.println(list.size());
		
		for(int i=0; i<list.size(); i++) {
			System.out.println(list.get(i).getText());
		}
